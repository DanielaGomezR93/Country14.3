            report_type="qweb-pdf"
                menu="False"
        />

        <record id="country_qr_code.print_qr" model="ir.actions.report">
            <field name="paperformat_id" ref="country_qr_code.customer_badge_paperformat"/>
        </record>

	    <record id="country_qr_code.print_qr_invitation" model="ir.actions.report">
            <field name="paperformat_id" ref="country_qr_code.customer_badge_paperformat"/>
        </record>

    </data>
