<odoo>
    <data>
        <record id="partner_insolvent_list_country" model="ir.ui.view">
            <field name="name">Listado en mora</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <tree edit="false" string="Listado en mora" create="false" delete="false">
                    <field name="image_medium" widget="image" class="oe_avatar"/>
                    <field name="action_number"/>
                    <field name="name"/>
                    <field name="vat" string="Documento de identificación"/>
                    <field name="other_doc_id"/>                   
                </tree>
            </field>
        </record>


        <record id="action_insolvent_partner" model="ir.actions.act_window">
            <field name="name">Listado en mora</field>
            <field name="res_model">res.partner</field>
            <field name="binding_view_types">form</field>
            <field name="view_mode">tree,kanban</field>
            <field name="view_id" ref="partner_insolvent_list_country"/>
            <field name="domain">[('is_solvent','=',False), ('state_partner', 'in', ['active','holder'])]</field>
            <!--<field name="context">{'default_type': 'in_invoice', 'type': 'in_invoice', 'journal_type': 'purchase',"search_default_unpaid":True,"search_default_in_payment":True}</field>-->
        </record>
        <menuitem name="Listado en mora" action="action_insolvent_partner"
              id="menu_partnert_insolvent_list"
              parent="Country_Socios.menu_root"/>
            <!--invoke wizard-->

	    <record id="partner_list_country" model="ir.ui.view">
            <field name="name">Listado de socios y Familiares</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <tree edit="false" string="Listado de socios y Familiares" create="false" delete="false">
                    <field name="image_medium" string="Foto" widget="image" class="oe_avatar"/>
                    <field name="action_number"/>
                    <field name="name"/>
                    <field name="vat" string="Cédula"/>
                    <field name="type_relation" string="Parentesco"/>
                    <field name="can_access_club" string="Acceso al club"/>
                    <field name="parent_id" string="Socio"/>
                    <field name="is_solvent_related"/>
                </tree>
            </field>
        </record>

	    <record id="action_partner_list" model="ir.actions.act_window">
            <field name="name">Listado de socios y Familiares</field>
            <field name="res_model">res.partner</field>
            <field name="binding_view_types">form</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="partner_list_country"/>
		    <field name="domain">['|', ('action_number', '!=', False), ('parent_id', '!=', False)]</field>
        </record>

	    <menuitem name="Listado de socios y familiares" action="action_partner_list"
              id="menu_partnert_list"
              parent="Country_Socios.menu_root"/>

    </data>
</odoo>