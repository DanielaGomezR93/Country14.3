<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
		<record id="tree_associates_country" model="ir.ui.view">
            <field name="name">res.partner.associate.tree</field>
            <field name="model">res.partner</field>
            <field name="arch" type="xml">
                <tree  string="Asociados" create="false">
                    <!--<field name="associate_parent"/>-->
                    <field name="associate_action"/>
                    <field name="action_number"/>
                    <field name="name" />
                    <field name="vat" />
                    <field name="member_gender" />
                    <field name="email" />
                    <field name="type_relation" string="Parentesco"/>
                    <field name="phone" />
                    <field name="mobile" />
                    <field name="end_date_family" /><!--duda fecha fin, parentezco,carnet-->
                    <field name="state_partner" />
                    <field name="type_of_member" invisible="1"/>

                </tree>
            </field>
        </record>

		<record id="action_associates" model="ir.actions.act_window">
            <field name="name">Asociados</field>
            <field name="res_model">res.partner</field>
            <field name="binding_view_types">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('type_of_member','=','extention')]</field>
        </record>

        <record model="ir.actions.act_window.view" id="act_associates_list">
            <field eval="1" name="sequence"/>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="tree_associates_country"/>
            <field name="act_window_id" ref="action_associates"/>
           
        </record>

	    <menuitem name="Asociados" id="Country_Socios.associates" parent="Country_Socios.menu_root" action="action_associates" />
    </data>
</odoo>